n=5
for i in range(1,n+1):
    for j in range(i):
        print(i,end="")
    print(" ")
    
4]    1234
      123
      12
      1
      
n=5
for i in range(n):
    for j in range(1,n-i):
        print(j,end="")
    print("")      
    
 5]    55555 
       4444 
       333 
       22 
       1
       
n=5
for i in range(n,0,-1):
    for j in range(i):
        print(i,end="")
    print(" ")
    
  6]   55555 
       5555 
       555 
       55 
       5
       
n=6
k=5
for i in range(1,n):
    for j in range(n-i):
        print(k,end="")
    print(" ")
    

